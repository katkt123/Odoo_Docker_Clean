common_taxes_data.reduce((sum,t)=>sum+t.raw_tax_amount,0);first_tax_details.total_included=first_tax_details.total_excluded+
first_tax_details.delta_total_excluded+
common_taxes_data.reduce((sum,t)=>sum+t.tax_amount,0);const second_tax_details={raw_total_excluded_currency:tax_data_to_remove.raw_tax_amount_currency,raw_total_excluded:tax_data_to_remove.raw_tax_amount,total_excluded_currency:tax_data_to_remove.tax_amount_currency,total_excluded:tax_data_to_remove.tax_amount,delta_total_excluded_currency:0.0,delta_total_excluded:0.0,raw_total_included_currency:tax_data_to_remove.raw_tax_amount_currency,raw_total_included:tax_data_to_remove.raw_tax_amount,total_included_currency:tax_data_to_remove.tax_amount_currency,total_included:tax_data_to_remove.tax_amount,taxes_data:[],};const target_factors=[{factor:first_tax_details.raw_total_excluded_currency,tax_details:first_tax_details,},{factor:second_tax_details.raw_total_excluded_currency,tax_details:second_tax_details,},];for(const remaining_tax_data of remaining_taxes_data){let first_tax_data;if(tax_data_to_remove.taxes.includes(remaining_tax_data.tax)){const new_remaining_taxes_data=this.split_tax_data(base_line,remaining_tax_data,company,target_factors);first_tax_data=new_remaining_taxes_data[0];second_tax_details.taxes_data.push(new_remaining_taxes_data[1]);second_tax_details.raw_total_included_currency+=new_remaining_taxes_data[1].raw_tax_amount_currency;second_tax_details.raw_total_included+=new_remaining_taxes_data[1].raw_tax_amount;second_tax_details.total_included_currency+=new_remaining_taxes_data[1].tax_amount_currency;second_tax_details.total_included+=new_remaining_taxes_data[1].tax_amount;}else{first_tax_data=remaining_tax_data;}
first_tax_details.taxes_data.push(first_tax_data);first_tax_details.raw_total_included_currency+=first_tax_data.raw_tax_amount_currency;first_tax_details.raw_total_included+=first_tax_data.raw_tax_amount;first_tax_details.total_included_currency+=first_tax_data.tax_amount_currency;first_tax_details.total_included+=first_tax_data.tax_amount;}
const first_taxes=first_tax_details.taxes_data.map((tax_data)=>tax_data.tax);const first_base_line=this.prepare_base_line_for_taxes_computation(base_line,{tax_ids:first_taxes,tax_details:first_tax_details,});const second_taxes=second_tax_details.taxes_data.map((tax_data)=>tax_data.tax);const second_base_line=this.prepare_base_line_for_taxes_computation(base_line,{tax_ids:second_taxes,price_unit:(second_tax_details.raw_total_excluded_currency+
second_tax_details.taxes_data.filter((t)=>t.tax.price_include).reduce((sum,t)=>sum+t.raw_tax_amount_currency,0))/(base_line.quantity||1.0),tax_details:second_tax_details,_removed_tax_data:tax_data_to_remove,});to_process.unshift([index,first_base_line,taxes_to_exclude],[index,second_base_line,taxes_to_exclude]);}
const final_base_lines=[];new_base_lines_list.forEach((new_base_lines)=>{new_base_lines[0].removed_taxes_data_base_lines=new_base_lines.slice(1);final_base_lines.push(new_base_lines[0]);});return final_base_lines;},turn_removed_taxes_into_new_base_lines(base_lines,company,{grouping_function=null,aggregate_function=null}={}){let extra_base_lines=[];for(const base_line of base_lines){extra_base_lines=extra_base_lines.concat(base_line.removed_taxes_data_base_lines||[]);}
return this.reduce_base_lines_with_grouping_function(extra_base_lines,{grouping_function:grouping_function,aggregate_function:aggregate_function,});},};return __exports;});;

/* /payment/static/src/interactions/express_checkout.js */
odoo.define('@payment/interactions/express_checkout',['@web/core/registry','@web/public/interaction'],function(require){'use strict';let __exports={};const{registry}=require('@web/core/registry');const{Interaction}=require('@web/public/interaction');const ExpressCheckout=__exports.ExpressCheckout=class ExpressCheckout extends Interaction{static selector='form[name="o_payment_express_checkout_form"]';setup(){this.paymentContext={};Object.assign(this.paymentContext,this.el.dataset);this.paymentContext.shippingInfoRequired=!!this.paymentContext.shippingInfoRequired;}
async willStart(){const expressCheckoutForm=this._getExpressCheckoutForm();if(expressCheckoutForm){await this._prepareExpressCheckoutForm(expressCheckoutForm.dataset);}}
