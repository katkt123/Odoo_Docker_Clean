Responses&&composer.targetThread&&store.env.services["mail.suggestion"].getSupportedDelimiters(composer.targetThread).find(([delimiter])=>delimiter==="::"),icon:"fa fa-file-text-o",name:_t("Insert a Canned response"),onSelected:({owner},ev)=>owner.onClickInsertCannedResponse(ev),sequence:5,});const ComposerAction=__exports.ComposerAction=class ComposerAction extends Action{composerFn;constructor({composer}){super(...arguments);this.composerFn=typeof composer==="function"?composer:()=>composer;}
