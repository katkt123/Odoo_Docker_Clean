odoo.define('@website/interactions/popup/no_backdrop_popup',['@web/public/interaction','@web/core/registry','@web/core/utils/scrolling'],function(require){'use strict';let __exports={};const{Interaction}=require("@web/public/interaction");const{registry}=require("@web/core/registry");const{isScrollableY}=require("@web/core/utils/scrolling");const NoBackdropPopup=__exports.NoBackdropPopup=class NoBackdropPopup extends Interaction{static selector=".s_popup_no_backdrop";dynamicContent={_root:{"t-on-shown.bs.modal":this.addModalNoBackdropEvents,"t-on-hide.bs.modal":this.removeModalNoBackdropEvents,},};setup(){this.throttledUpdateScrollbar=this.throttled(this.updateScrollbar);this.removeResizeListener=null;this.resizeObserver=null;}
