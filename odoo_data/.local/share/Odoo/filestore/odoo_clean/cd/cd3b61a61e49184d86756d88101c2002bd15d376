                    <input type="radio" t-attf-id="ptav-{{ptav.id}}-input" t-att-value="ptav.id" t-att-name="'ptal-' + this.props.id" t-att-checked="this.props.selected_attribute_value_ids.includes(ptav.id)" t-on-change="updateSelectedPTAV"/>
                </label>
            </li>
        </ul>
    </t>
    `);
