odoo.define('@website_sale_wishlist/interactions/product_wishlist',['@web/core/network/rpc','@web/core/registry','@web/core/utils/urls','@web/public/interaction','@website_sale_wishlist/js/website_sale_wishlist_utils'],function(require){'use strict';let __exports={};const{rpc}=require('@web/core/network/rpc');const{registry}=require('@web/core/registry');const{redirect}=require('@web/core/utils/urls');const{Interaction}=require('@web/public/interaction');const wishlistUtils=require('@website_sale_wishlist/js/website_sale_wishlist_utils')[Symbol.for("default")];const ProductWishlist=__exports.ProductWishlist=class ProductWishlist extends Interaction{static selector='.wishlist-section';dynamicContent={'.o_wish_rm':{'t-on-click':this.removeProduct},'.o_wish_add':{'t-on-click':this.addToCart},};async removeProduct(ev){await this._removeProduct(ev.currentTarget);}
