odoo.define('@website_sale_comparison/js/product_row/product_row',['@odoo/owl','@web/core/currency','@website_sale_comparison/js/website_sale_comparison_utils'],function(require){'use strict';let __exports={};const{Component}=require('@odoo/owl');const{formatCurrency}=require('@web/core/currency');const comparisonUtils=require('@website_sale_comparison/js/website_sale_comparison_utils')[Symbol.for("default")];const ProductRow=__exports.ProductRow=class ProductRow extends Component{static template='website_sale_comparison.ProductRow';static props={id:Number,display_name:String,website_url:String,image_url:String,price:Number,strikethrough_price:{type:Number,optional:true},prevent_zero_price_sale:Boolean,currency_id:Number,};removeProduct(){comparisonUtils.removeComparisonProduct(this.props.id,this.env.bus);comparisonUtils.enableDisabledProducts([this.props.id],false);}
