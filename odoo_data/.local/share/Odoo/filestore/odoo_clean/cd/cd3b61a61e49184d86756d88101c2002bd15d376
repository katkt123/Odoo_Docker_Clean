_canSubmit(){const paymentForm=document.querySelector('#o_payment_form');if(!paymentForm){return true;}
return document.querySelectorAll('input[name="o_payment_radio"]:checked').length===1;}
_setEnabled(){this.paymentButton.disabled=false;}
_disable(){this.paymentButton.disabled=true;}
_hide(){this.paymentButton.classList.add('d-none');}
_show(){this.paymentButton.classList.remove('d-none');}}
