                                <span t-attf-class="o_we_full_url text-muted o_we_webkit_box" t-attf-title="{{state.url}}">
                                    <t t-if="state.url" t-esc="state.url"/>
                                    <t t-else="">No URL specified</t>
                                </span>
                            </div>
                        </div>
