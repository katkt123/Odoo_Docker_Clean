/* /delivery/static/src/js/location_selector/location_selector_dialog/location_selector_dialog.js */
odoo.define('@delivery/js/location_selector/location_selector_dialog/location_selector_dialog',['@delivery/js/location_selector/location_list/location_list','@delivery/js/location_selector/map_container/map_container','@odoo/owl','@web/core/browser/browser','@web/core/dialog/dialog','@web/core/l10n/translation','@web/core/network/rpc','@web/core/utils/timing'],function(require){'use strict';let __exports={};const{LocationList}=require('@delivery/js/location_selector/location_list/location_list');const{MapContainer}=require('@delivery/js/location_selector/map_container/map_container');const{Component,onMounted,onWillUnmount,useEffect,useState}=require('@odoo/owl');const{browser}=require('@web/core/browser/browser');const{Dialog}=require('@web/core/dialog/dialog');const{_t}=require('@web/core/l10n/translation');const{rpc}=require('@web/core/network/rpc');const{useDebounced}=require('@web/core/utils/timing');const LocationSelectorDialog=__exports.LocationSelectorDialog=class LocationSelectorDialog extends Component{static components={Dialog,LocationList,MapContainer};static template='delivery.locationSelector.dialog';static props={orderId:Number,zipCode:String,selectedLocationId:{type:String,optional:true},save:Function,close:Function,};static defaultProps={selectedLocationId:false,};setup(){this.state=useState({locations:[],error:false,viewMode:'list',zipCode:this.props.zipCode,selectedLocationId:String(this.props.selectedLocationId),isSmall:this.env.isSmall,});this.getLocationUrl='/delivery/get_pickup_locations';this.debouncedOnResize=useDebounced(this.updateSize,300);this.debouncedSearchButton=useDebounced((zipCode)=>{this.state.locations=[];this._updateLocations(zipCode);},300);onMounted(()=>{browser.addEventListener('resize',this.debouncedOnResize);this.updateSize();});onWillUnmount(()=>browser.removeEventListener('resize',this.debouncedOnResize));useEffect((zipCode)=>{this._updateLocations(zipCode)
return()=>{this.state.locations=[]};},()=>[this.state.zipCode]);}
async _getLocations(zip){return rpc(this.getLocationUrl,{order_id:this.props.orderId,zip_code:zip});}
async _updateLocations(zip){this.state.error=false;const{pickup_locations,error}=await this._getLocations(zip);if(error){this.state.error=error;console.error(error);}else{this.state.locations=pickup_locations;if(!this.state.locations.find(l=>String(l.id)===this.state.selectedLocationId)){this.state.selectedLocationId=this.state.locations[0]?String(this.state.locations[0].id):false;}}}
get showListView(){return this.state.locations.length!==1;}
get selectedLocation(){return this.state.locations.find(l=>String(l.id)===this.state.selectedLocationId);}
setSelectedLocation(locationId){this.state.selectedLocationId=String(locationId);}
async validateSelection(){if(!this.state.selectedLocationId)return;const selectedLocation=this.state.locations.find(l=>String(l.id)===this.state.selectedLocationId);await this.props.save(selectedLocation);this.props.close();}
get mobileComponent(){if(this.state.viewMode==='map')return MapContainer;return LocationList;}
get title(){if(this.state.locations.length===1){return _t("Pickup Location")}
return _t("Choose a pick-up point");}
get validationButtonLabel(){return _t("Choose this location");}
get postalCodePlaceholder(){return _t("Your postal code");}
get listViewButtonLabel(){return _t("List view");}
get mapViewButtonLabel(){return _t("Map view");}
get errorMessage(){return _t("No result");}
get loadingMessage(){return _t("Loading...");}
updateSize(){this.state.isSmall=this.env.isSmall;}}
return __exports;});;

/* /delivery/static/src/js/location_selector/map/map.js */
odoo.define('@delivery/js/location_selector/map/map',['@odoo/owl','@web/core/utils/render'],function(require){'use strict';let __exports={};const{Component,useEffect,useRef}=require('@odoo/owl');const{renderToString}=require('@web/core/utils/render');const Map=__exports.Map=class Map extends Component{static template='delivery.locationSelector.map';static props={locations:{type:Array,element:{type:Object,values:{id:String,name:String,openingHours:{type:Object,values:{type:Array,element:String,optional:true,},},street:String,city:String,zip_code:String,state:{type:String,optional:true},country_code:String,additional_data:{type:Object,optional:true},latitude:String,longitude:String,}},},selectedLocationId:[String,{value:false}],setSelectedLocation:Function,};setup(){this.leafletMap=null;this.markers=[];this.mapRef=useRef('map');useEffect(()=>{this.leafletMap=L.map(this.mapRef.el,{zoom:13,});this.leafletMap.attributionControl.setPrefix('<a href="https://leafletjs.com" title="A JavaScript library for interactive maps">Leaflet</a>');L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png',{maxZoom:19,attribution:"&copy; <a href='http://www.openstreetmap.org/copyright'>OpenStreetMap</a>"}).addTo(this.leafletMap);return()=>{this.leafletMap.remove();}},()=>[]);useEffect((locations)=>{this.leafletMap.invalidateSize();},()=>[this.props.locations]);useEffect((locations,selectedLocationId)=>{this.addMarkers(locations);const selectedLocation=locations.find(l=>String(l.id)===selectedLocationId);if(selectedLocation){this.leafletMap.panTo([selectedLocation.latitude,selectedLocation.longitude],{animate:true});}
return()=>{this.removeMarkers();};},()=>[this.props.locations,this.props.selectedLocationId]);}
addMarkers(locations){for(const loc of locations){const isSelected=String(loc.id)===this.props.selectedLocationId
const iconInfo={className:isSelected?'o_location_selector_marker_icon_selected':'o_location_selector_marker_icon',html:renderToString('delivery.locationSelector.map.marker',{number:locations.indexOf(loc)+1},),iconSize:[30,40],iconAnchor:[15,40],};const marker=L.marker([loc.latitude,loc.longitude],{icon:L.divIcon(iconInfo),title:locations.indexOf(loc)+1,},);if(isSelected)marker.setZIndexOffset(100);marker.addTo(this.leafletMap);marker.addEventListener('click',()=>{this.props.setSelectedLocation(loc.id);});this.markers.push(marker);}}
removeMarkers(){for(const marker of this.markers){marker.removeEventListener();this.leafletMap.removeLayer(marker);}
this.markers=[];}
get selectedLocation(){return this.props.locations.find(l=>String(l.id)===this.props.selectedLocationId)}}
return __exports;});;

