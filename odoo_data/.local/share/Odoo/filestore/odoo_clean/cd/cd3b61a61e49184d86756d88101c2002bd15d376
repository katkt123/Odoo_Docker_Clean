const prettifiedText=isHtmlEmpty(this.composerHtml)?"":convertBrToLineBreak(this.composerHtml);if(this.composerText!==prettifiedText){this.updateFrom="html";this.composerText=prettifiedText;}},});thread=fields.One("Thread");selection={start:0,end:0,direction:"none",};forceCursorMove;isFocused=fields.Attr(false,{onUpdate(){if(this.thread){if(this.isFocused){this.thread.isFocusedCounter++;}else{this.thread.isFocusedCounter--;}}},});autofocus=0;replyToMessage=fields.One("mail.message");updateFrom=undefined;get syncHtmlWithMessage(){return this.message&&!this.isDirty;}
