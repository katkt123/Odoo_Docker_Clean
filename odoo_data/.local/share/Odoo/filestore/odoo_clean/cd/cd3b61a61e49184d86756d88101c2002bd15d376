if(partner?.eq(store.self_partner)||guest?.eq(store.self_guest)){const isOnline=presence.getInactivityPeriod()<AWAY_DELAY;if((presence_status==="away"&&isOnline)||presence_status==="offline"){updateBusPresence();}}});presence.bus.addEventListener("presence",()=>{if(lastSentInactivity>=AWAY_DELAY){updateBusPresence();}
startAwayTimeout();});return{updateBusPresence};},};registry.category("services").add("im_status",imStatusService);return __exports;});;

