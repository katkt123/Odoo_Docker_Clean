getHeaderHeight(){if(this.hideEl){if(this.isSmall()){return this.el.getBoundingClientRect().height;}
if(this.hideEl.classList.contains("hidden")){return this.hideElHeight+this.el.getBoundingClientRect().height;}
this.hideElHeight=this.hideEl?.getBoundingClientRect().height;}
return this.el.getBoundingClientRect().height;}}
