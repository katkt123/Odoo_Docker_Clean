/* /sale/static/src/js/badge_extra_price/badge_extra_price.js */
odoo.define('@sale/js/badge_extra_price/badge_extra_price',['@odoo/owl','@web/core/currency'],function(require){'use strict';let __exports={};const{Component}=require("@odoo/owl");const{formatCurrency}=require("@web/core/currency");const BadgeExtraPrice=__exports.BadgeExtraPrice=class BadgeExtraPrice extends Component{static template="sale.BadgeExtraPrice";static props={price:Number,currencyId:Number,};getFormattedPrice(){return formatCurrency(Math.abs(this.props.price),this.props.currencyId);}}
