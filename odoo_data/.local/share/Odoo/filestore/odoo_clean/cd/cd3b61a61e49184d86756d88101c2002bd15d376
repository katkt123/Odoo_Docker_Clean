const units=Object.entries(duration.toObject()).filter(([unit,amount])=>amount!=0).map(([unit,amount])=>unit);return _t("Call lasted %(duration)s.",{duration:duration.shiftTo(...units).toHuman({unitDisplay:"short"}),});}}
return __exports;});;

