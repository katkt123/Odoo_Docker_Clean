odoo.define('@website/interactions/carousel/carousel_slider',['@web/public/interaction','@web/core/registry','@website/utils/images'],function(require){'use strict';let __exports={};const{Interaction}=require("@web/public/interaction");const{registry}=require("@web/core/registry");const{onceAllImagesLoaded}=require("@website/utils/images");const CarouselSlider=__exports.CarouselSlider=class CarouselSlider extends Interaction{static selector=".carousel";dynamicContent={_root:{"t-on-slide.bs.carousel":this.onSlideCarousel,"t-on-slid.bs.carousel":this.onSlidCarousel,},img:{"t-on-load":this.computeMaxHeight,},_window:{"t-on-resize":this.debounced(this.computeMaxHeight,250),},".carousel-item":{"t-att-style":()=>({"min-height":this.maxHeight?`${this.maxHeight}px`:"",}),},".slide-link":{"t-att-class":()=>({"d-none":!this.showClickableSlideLinks})},};carouselOptions=undefined;showClickableSlideLinks=true;setup(){this.maxHeight=undefined;this.hasInterval=![undefined,"false","0"].includes(this.el.dataset.bsInterval);if(!["true","carousel","false"].includes(this.el.dataset.bsRide)){this.el.dataset.bsRide=this.hasInterval?"carousel":"false";}
