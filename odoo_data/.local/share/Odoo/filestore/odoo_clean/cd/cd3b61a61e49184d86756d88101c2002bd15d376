odoo.define('@delivery/js/location_selector/map_container/map_container',['@delivery/js/location_selector/location_schedule/location_schedule','@delivery/js/location_selector/map/map','@odoo/owl','@web/core/assets','@web/core/l10n/translation'],function(require){'use strict';let __exports={};const{LocationSchedule}=require('@delivery/js/location_selector/location_schedule/location_schedule');const{Map}=require('@delivery/js/location_selector/map/map');const{Component,onWillStart,useState}=require('@odoo/owl');const{AssetsLoadingError,loadCSS,loadJS}=require('@web/core/assets');const{_t}=require('@web/core/l10n/translation');const MapContainer=__exports.MapContainer=class MapContainer extends Component{static components={LocationSchedule,Map};static template='delivery.locationSelector.mapContainer';static props={locations:{type:Array,element:{type:Object,values:{id:String,name:String,openingHours:{type:Object,values:{type:Array,element:String,optional:true,},},street:String,city:String,zip_code:String,state:{type:String,optional:true},country_code:String,additional_data:{type:Object,optional:true},latitude:String,longitude:String,}},},selectedLocationId:[String,{value:false}],setSelectedLocation:Function,validateSelection:Function,};setup(){this.state=useState({shouldLoadMap:false,});onWillStart(async()=>{try{await Promise.all([loadJS('https://unpkg.com/leaflet@1.9.4/dist/leaflet.js'),loadCSS('https://unpkg.com/leaflet@1.9.4/dist/leaflet.css'),])
this.state.shouldLoadMap=true;}catch(error){if(!(error instanceof AssetsLoadingError)){throw error;}}});}
getCityAndZipCode(selectedLocation){return`${selectedLocation.zip_code} ${selectedLocation.city}`;}
