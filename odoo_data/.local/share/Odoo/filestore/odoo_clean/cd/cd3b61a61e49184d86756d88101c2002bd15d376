function updateCartSummary(data){if(data['website_sale.shorter_cart_summary']){const shorterCartSummaryEl=document.querySelector('.o_wsale_shorter_cart_summary');setElementContent(shorterCartSummaryEl,markup(data['website_sale.shorter_cart_summary']));}
