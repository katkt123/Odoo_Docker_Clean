const itemsToLoad=nextItemElsToLoad.concat(prevItemElsToLoad);for(const carouselItemEl of itemsToLoad){const imageEls=carouselItemEl.querySelectorAll("img[loading='lazy']");for(const imageEl of imageEls){imageEl.removeAttribute("loading");}}}}
