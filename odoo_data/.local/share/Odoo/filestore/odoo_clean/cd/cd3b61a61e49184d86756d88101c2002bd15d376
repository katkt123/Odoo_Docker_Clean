n=__exports.rejectAction={btnClass:({owner,thread})=>attClassObjectToString({"pe-2 rounded-pill":typeof thread?.useCameraByDefault==="boolean","mx-1":!owner.env.inCallInvitation&&typeof thread?.useCameraByDefault==="boolean",}),condition:({thread})=>thread?.self_member_id?.rtc_inviting_session_id,disabledCondition:({store})=>store.rtc?.state.hasPendingRequest,icon:"oi oi-close",inlineName:({owner,thread})=>!owner.env.inCallInvitation&&typeof thread?.useCameraByDefault==="boolean"?_t("Reject"):undefined,name:_t("Reject"),onSelected:({store,thread})=>{if(store.rtc.state.hasPendingRequest){return;}
