odoo.define('@website_mail/interactions/follow',['@web/public/interaction','@web/core/registry','@web/core/network/rpc','@google_recaptcha/js/recaptcha'],function(require){'use strict';let __exports={};const{Interaction}=require("@web/public/interaction");const{registry}=require("@web/core/registry");const{rpc}=require("@web/core/network/rpc");const{ReCaptcha}=require("@google_recaptcha/js/recaptcha");const Follow=__exports.Follow=class Follow extends Interaction{static selector="#wrapwrap";static selectorHas=".js_follow";dynamicContent={".js_follow > .d-none":{"t-att-class":()=>({"d-none":false}),},".js_follow_btn, .js_unfollow_btn":{"t-on-click.prevent.withTarget":this.onToggleFollowClick,},};setup(){this.isUser=false;this.recaptcha=new ReCaptcha();}
