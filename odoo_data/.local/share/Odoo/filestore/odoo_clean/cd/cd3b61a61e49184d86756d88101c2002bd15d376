selection=this.dependencies.selection.getEditableSelection();let startNode;let insertBefore=false;if(selection.startContainer.nodeType===Node.TEXT_NODE){insertBefore=!selection.startOffset;splitTextNode(selection.startContainer,selection.startOffset,DIRECTIONS.LEFT);startNode=selection.startContainer;}
