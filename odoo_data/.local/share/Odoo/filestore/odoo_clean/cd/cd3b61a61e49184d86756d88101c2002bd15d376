let result;try{result=await ormService.call("res.users.identitycheck","run_check",[checkId],{'context':{'password':inputEl.value}},);}catch{inputEl.classList.add("is-invalid");inputEl.setCustomValidity(_t("Check failed"));inputEl.reportValidity();return false;}
resolve(result);return true;},cancel:()=>{},onInput:({inputEl})=>{inputEl.classList.remove("is-invalid");inputEl.setCustomValidity("");},});});});}
