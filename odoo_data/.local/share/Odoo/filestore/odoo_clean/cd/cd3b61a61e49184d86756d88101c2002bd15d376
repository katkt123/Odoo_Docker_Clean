odoo.define('@website/snippets/s_map/map',['@web/public/interaction','@web/core/registry','@website/js/utils'],function(require){'use strict';let __exports={};const{Interaction}=require("@web/public/interaction");const{registry}=require("@web/core/registry");const{generateGMapLink,generateGMapIframe}=require("@website/js/utils");const Map=__exports.Map=class Map extends Interaction{static selector=".s_map";start(){if(!this.el.querySelector(".s_map_embedded")){const dataset=this.el.dataset;if(dataset.mapAddress){const iframeEl=generateGMapIframe();this.el.querySelector(".s_map_color_filter").before(iframeEl);this.services.website_cookies.manageIframeSrc(iframeEl,generateGMapLink(dataset));}}}}
